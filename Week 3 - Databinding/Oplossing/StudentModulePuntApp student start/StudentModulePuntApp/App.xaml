<Application
    x:Class="StudentModulePuntApp.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:StudentModulePuntApp"
    xmlns:repositories="using:StudentModulePuntApp.Repositories"
    RequestedTheme="Light">

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles/ResourceDictionary.xaml" />
                <ResourceDictionary Source="DataTemplates/DataTemplates.xaml" />
            </ResourceDictionary.MergedDictionaries>
            <repositories:StudentRepository x:Key="studrep" />
        </ResourceDictionary>
    </Application.Resources>
</Application>
